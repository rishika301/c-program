//prime number checker
#include<stdio.h>
int main()
{
    int n,flag=0,i=2;
    printf("enter the value of a");
    scanf("%d",&n);
   while(i<=n/2)
   {
       if (n%i==0);
       {
           flag=1;
           break ;
           }
            i++;
       }
       if(flag==0)
        printf("it is prime number");
       else
        printf("it is not prime number");
        return 0;
   }
